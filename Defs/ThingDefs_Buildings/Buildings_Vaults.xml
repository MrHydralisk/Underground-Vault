<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef Name="UVVaultBase" ParentName="BuildingBase" Abstract="True">
		<thingClass>UndergroundVault.Building_UVVault</thingClass>
		<altitudeLayer>Floor</altitudeLayer>
		<rotatable>false</rotatable>
		<destroyable>false</destroyable>
		<useHitPoints>false</useHitPoints>
		<tickerType>Normal</tickerType>
		<selectable>true</selectable>
		<terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
		<neverOverlapFloors>true</neverOverlapFloors>
		<blockPlants>true</blockPlants>
		<building>
			<isEdifice>false</isEdifice>
			<claimable>false</claimable>
			<deconstructible>false</deconstructible>
		</building>
	</ThingDef>



	<ThingDef ParentName="UVVaultBase">
		<defName>UVVaultCemeteryS</defName>
		<label>cemetery underground vault</label>
		<description>Cemetery underground vault  for storing sarcophagus. Size S.</description>
		<graphicData>
			<texPath>Things/Building/UVVault1x2</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(3,4)</drawSize>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<size>(3,2)</size>
		<modExtensions>
			<li Class="UndergroundVault.VaultExtension">
				<TerminalDef>UVTerminalCemeteryS</TerminalDef>
				<FloorSize>3</FloorSize>
			</li>
		</modExtensions>
	</ThingDef>
  
