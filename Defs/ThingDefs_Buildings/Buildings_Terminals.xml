<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <ThingDef Name="UVTerminalBase" ParentName="BuildingBase" Abstract="True">
      <castEdgeShadows>true</castEdgeShadows>
      <fillPercent>0.5</fillPercent>
          <rotatable>false</rotatable>
      <!-- <tickerType>Normal</tickerType> -->
      <!-- <drawerType>MapMeshAndRealTime</drawerType> -->
      <canOverlapZones>false</canOverlapZones>
      <constructionSkillPrerequisite>5</constructionSkillPrerequisite>
      <altitudeLayer>Building</altitudeLayer>
      <passability>PassThroughOnly</passability>
      <blockWind>false</blockWind>
      <terrainAffordanceNeeded>Diggable</terrainAffordanceNeeded>
      <staticSunShadowHeight>0.3</staticSunShadowHeight>
      <comps>
        <li Class="CompProperties_Mannable"/>
        <li Class="CompProperties_Flickable"/>
        <!-- <li Class="CompProperties_Breakdownable"/> -->
      </comps>
    </ThingDef>
    
    <ThingDef ParentName="UVTerminalBase">
      <defName>UVTerminalCemetery</defName>
      <label>cemetery vault terminal</label>
      <description>Terminal for cemetery vault management and access point to it.</description>
      <thingClass>UndergroundVault.Building_UVTerminalCemetery</thingClass>
      <graphicData>
        <texPath>Things/Building/UVTerminal1x2</texPath>
        <graphicClass>Graphic_Single</graphicClass>
        <drawSize>(5,4)</drawSize>
        <damageData>
          <cornerTL>Damage/Corner</cornerTL>
          <cornerTR>Damage/Corner</cornerTR>
          <cornerBL>Damage/Corner</cornerBL>
          <cornerBR>Damage/Corner</cornerBR>
        </damageData>
      </graphicData>
      <size>(3,2)</size>
      <tickerType>Normal</tickerType>
      <!-- <tickerType>Normal</tickerType> -->
      <constructionSkillPrerequisite>5</constructionSkillPrerequisite>    
      <hasInteractionCell>True</hasInteractionCell>
      <interactionCellOffset>(1,0,-1)</interactionCellOffset>
      <building>
        <!-- <ai_chillDestination>false</ai_chillDestination> -->
      </building>
      <inspectorTabs>
        <li>UndergroundVault.ITab_UVTerminalCemetery_Inventory</li>
      </inspectorTabs>
      <comps>
        <li Class="CompProperties_Power">
          <!-- <compClass>UndergroundVault.CompPowerUVTerminal</compClass> -->
          <compClass>CompPowerTrader</compClass>
          <basePowerConsumption>150</basePowerConsumption>
        </li>
      </comps>
      <!-- <costStuffCount>60</costStuffCount> -->
      <designationCategory>Misc</designationCategory>
      <researchPrerequisites>
        <li>ComplexFurniture</li>
      </researchPrerequisites>
      <!-- <placeWorkers> -->
        <!-- <li>PlaceWorker_MeditationOffsetBuildingsNear</li> -->
      <!-- </placeWorkers> -->
      <!-- <ritualFocus> -->
        <!-- <spectateDistance>1</spectateDistance> -->
        <!-- <allowedSpectateSides>Vertical</allowedSpectateSides> -->
      <!-- </ritualFocus> -->
      <modExtensions>
        <li Class="UndergroundVault.BuildingUpgradesExtension">
          <ConstructionOffset>
            <li>(1,0,0)</li>
            <li>(1,0,1)</li>
            <li>(-1,0,1)</li>
            <li>(-1,0,0)</li>
          </ConstructionOffset>
          <DrawOffset>
            <li>(0.109,0,0.531)</li>
            <li>(0.109,0,0.156)</li>
            <li>(-0.109,0,0.156)</li>
            <li>(-0.109,0,0.531)</li>
          </DrawOffset>
          <AvailableUpgrades>
            <li>
              <uiIconPath>Icons/UVUpgradeCrematorium_Icon</uiIconPath>
              <upgradeDef>UVUpgradeCrematorium</upgradeDef>
            </li>
            <li>
              <uiIconPath>Icons/UVUpgradeDeepDrill_Icon</uiIconPath>
              <upgradeDef>UVUpgradeDeepDrill</upgradeDef>
            </li>
            <li>
              <uiIconPath>Icons/UVUpgradeStorageEfficiency_Icon</uiIconPath>
              <upgradeDef>UVUpgradeStorageEfficiency</upgradeDef>
              <maxAmount>3</maxAmount>
            </li>
            <li>
              <uiIconPath>Icons/UVUpgradeAI_Icon</uiIconPath>
              <upgradeDef>UVUpgradeAI</upgradeDef>
            </li>
            <!-- <li>
              <uiIconPath>Icons/UVUpgradeFreezer_Icon</uiIconPath>
              <upgradeDef>UVUpgradeFreezer</upgradeDef>
            </li> -->
            <li>
              <uiIconPath>Icons/UVUpgradePlatformSpeed_Icon</uiIconPath>
              <upgradeDef>UVUpgradePlatformSpeed</upgradeDef>
              <maxAmount>2</maxAmount>
            </li>
            <!-- <li>
              <uiIconPath>UI/Commands/DesirePower</uiIconPath>
              <upgradeDef>UVUpgradePowerEfficiency</upgradeDef>
              <maxAmount>2</maxAmount>
            </li> -->
          </AvailableUpgrades>
        </li>
        <li Class="UndergroundVault.TerminalExtension">
          <VaultDef>UVVaultCemetery</VaultDef>
        </li>
      </modExtensions>
    </ThingDef>
  
